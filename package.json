{
  "name": "koa2-swagger-ui",
  "version": "0.0.0-placeholder",
  "description": "Swagger UI middleware for koa",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "start": "ts-node test/example.ts",
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "postbuild": "cp lib/index.hbs dist && cp lib/favicon-32x32.png dist && cp lib/favicon-16x16.png dist && cp package.json dist && cp LICENSE dist && cp README.md dist",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "handlebars": "^4.1.0",
    "lodash": "^4.17.11",
    "read-pkg-up": "^4.0.0"
  },
  "peerDependencies": {
    "koa": "^2.5.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.4",
    "@types/koa": "2.0.48",
    "@types/koa-router": "7.0.39",
    "@types/lodash": "4.14.121",
    "@types/node": "11.9.4",
    "@types/read-pkg-up": "3.0.1",
    "@types/supertest": "2.0.7",
    "inquirer": "6.2.2",
    "jest": "24.1.0",
    "koa": "2.7.0",
    "koa-router": "7.4.0",
    "microbundle": "0.9.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "supertest": "3.4.2",
    "swagger-ui-dist": "3.20.7",
    "ts-jest": "23.10.5",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-xo": "0.14.0",
    "typescript": "3.3.3"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true
  },
  "homepage": "https://github.com/scttcper/koa2-swagger-ui",
  "repository": "scttcper/koa2-swagger-ui",
  "keywords": [
    "swagger",
    "docs",
    "swagger-ui",
    "koa",
    "koa2",
    "middleware"
  ],
  "license": "MIT"
}
